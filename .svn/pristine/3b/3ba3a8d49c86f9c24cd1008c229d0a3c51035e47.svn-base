<include file="Public:head_v3"/>
<body>

<div id="theme-wrapper" class="rushToPurchase">

    <include file="Public:header_v3"/>
    <div id="page-wrapper" class="container">
        <div class="row">
            <include file="Public:sidebar_v3"/>
            <div id="content-wrapper">

                <div class="md-content">
                    <div class="modal-header">
                        <button class="md-close close">&times;</button>
                        <h4 class="modal-title">编辑减价模板</h4>
                    </div>
                    <form action="/index.php?m=BargainActivityTml&a=save"  method="post" role="form" id="add_form">
                        <div class="modal-body">
                            <div class="row">
                                <div class="form-group col-xs-4">
                                    <span class="label">团购id</span>

                                    <a href="javascript:void(0);" id="select_group_tml_id" class="modals_radio_select md-trigger btn btn-primary"
                                       data-inputId="groupbuytml_id" data-url="/index.php?s=/GroupBuyingTml/selectJson"
                                       data-title="选择团购模板" data-modal="modal_radio_select">选择团购模板</a>

                                    <input type="text" name="groupbuytml_id" class="form-control col-xs-2 modals_radio_select_input" id="groupbuytml_id" value="{$item.groupbuy_tml_id}"/>
                                </div>

                                <div class="form-group col-xs-12">
                                    <span class="label">团购名称:</span>  <span id="groupbuy_name" class="label"></span>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-5">
                                    <span class="label">初始价格:</span>
                                    <input type="text" name="begin_price" id="begin_price"
                                           class="form-control col-xs-6" value="{$item.begin_price}"/>
                                </div>

                                <div class="form-group col-xs-4">
                                    <span class="label">中间价格:</span><input type="text" name="mid_price" id="mid_price" class="form-control col-xs-3" value="{$item.mid_price}"/>
                                </div>

                                <div class="form-group col-xs-5">
                                    <span class="label">最低价格:</span><input type="text" name="min_price" id="min_price" class="form-control col-xs-6" value="{$item.min_price}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-5">
                                    <span class="label">减价类型:</span>
                                    <!--0无其它条件，1分享到朋友圈，2会员等级-->
                                    <select name="dec_sale_type" id="dec_sale_type">
                                        <option value="0" <if condition="$item.dec_sale_type eq 0">selected="selected"</if> >按最小减价值减</option>
                                        <option value="1" <if condition="$item.dec_sale_type eq 1">selected="selected"</if> >最小最大之间随机</option>
                                    </select>
                                </div>

                                <div class="form-group col-xs-3">
                                    <span class="label">单位:</span>
                                    <select name="unit_type" id="unit_type">
                                        <option value="0">元</option>
                                        <option value="1">克</option>
                                    </select>
                                </div>

                                <div class="form-group col-xs-3">
                                    <span class="label">单价:</span>
                                    <input type="text" name="unit_price" id="unit_price" class="form-control col-xs-3" value="{$item.unit_price}"/>
                                </div>

                                <div class="row">
                                    <div class="form-group col-xs-3">
                                        <!--条件参数（默认0，没有。例如：condition_type为2时，这个数字表示要求的会员等级）-->
                                        <span class="label">最小减价值:</span>
                                        <input type="text" name="sale_min" id="sale_min" class="form-control col-xs-3" value="{$item.sale_min}"/>
                                    </div>
                                    <div class="form-group col-xs-3">
                                        <!--条件参数（默认0，没有。例如：condition_type为2时，这个数字表示要求的会员等级）-->
                                        <span class="label">最大减价值:</span>
                                        <input type="text" name="sale_max" id="sale_max" class="form-control col-xs-3" value="{$item.sale_max}"/>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-xs-3">
                                        <!--条件参数（默认0，没有。例如：condition_type为2时，这个数字表示要求的会员等级）-->
                                        <span class="label">最小减值2:</span>
                                        <input type="text" name="sale_min1" id="sale_min1" class="form-control col-xs-3" value="{$item.sale_min1}"/>
                                    </div>
                                    <div class="form-group col-xs-3">
                                        <!--条件参数（默认0，没有。例如：condition_type为2时，这个数字表示要求的会员等级）-->
                                        <span class="label">最大减值2:</span>
                                        <input type="text" name="sale_max1" id="sale_max1" class="form-control col-xs-3" value="{$item.sale_max1}"/>
                                    </div>
                                </div>

                            </div>

                            <div class="row">
                                <div class="form-group col-xs-3">
                                    <span class="label">要求至少几个人才可以支付:</span><input type="text" name="req_min_partake_num" id="req_min_partake_num" class="form-control col-xs-3" value="{$item.req_min_partake_num}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动标题:</span><input type="text" name="activity_title" id="activity_title" class="form-control col-xs-8" value="{$item.activity_title}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">头像上的话:</span><input type="text" name="leader_title" id="leader_title" class="form-control col-xs-8" value="{$item.leader_title}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动背景URL:</span><input type="text" name="activity_bg_url" id="activity_bg_url" class="form-control col-xs-8" value="{$item.activity_bg_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动广告url:</span><input type="text" name="advert_img_url" id="advert_img_url" class="form-control col-xs-8" value="{$item.advert_img_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">产品说明文字:</span><input type="text" name="pruduct_desc_text" id="pruduct_desc_text" class="form-control col-xs-8" value="{$item.pruduct_desc_text}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">产品说明背景色rgb:</span>
                                    <input type="text" name="pruduct_desc_text_bg_r" id="pruduct_desc_text_bg_r" class="form-control col-xs-2" value="{$item.pruduct_desc_text_bg_r}"/>
                                    <input type="text" name="pruduct_desc_text_bg_g" id="pruduct_desc_text_bg_g" class="form-control col-xs-2" value="{$item.pruduct_desc_text_bg_g}"/>
                                    <input type="text" name="pruduct_desc_text_bg_b" id="pruduct_desc_text_bg_b" class="form-control col-xs-2" value="{$item.pruduct_desc_text_bg_b}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">产品说明文字色rgb:</span>
                                    <input type="text" name="pruduct_desc_text_r" id="pruduct_desc_text_r" class="form-control col-xs-2" value="{$item.pruduct_desc_text_r}"/>
                                    <input type="text" name="pruduct_desc_text_g" id="pruduct_desc_text_g" class="form-control col-xs-2" value="{$item.pruduct_desc_text_g}"/>
                                    <input type="text" name="pruduct_desc_text_b" id="pruduct_desc_text_b" class="form-control col-xs-2" value="{$item.pruduct_desc_text_b}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">人员列表前的文字背景URL:</span><input type="text" name="front_p_list_bg_url" id="front_p_list_bg_url" class="form-control col-xs-8" value="{$item.front_p_list_bg_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">人员列表前的说明文字:</span>
                                    <input type="text" name="front_p_list_text" id="front_p_list_text" class="form-control col-xs-2" value="{$item.front_p_list_text}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第一个人背景色rgb:</span>
                                    <input type="text" name="first_people_r" id="first_people_r" class="form-control col-xs-2" value="{$first_people_r}"/>
                                    <input type="text" name="first_people_g" id="first_people_g" class="form-control col-xs-2" value="{$first_people_g}"/>
                                    <input type="text" name="first_people_b" id="first_people_b" class="form-control col-xs-2" value="{$first_people_b}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第一个人左边icon:</span><input type="text" name="first_people_left_icon_url" id="first_people_left_icon_url" class="form-control col-xs-8" value="{$item.first_people_left_icon_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第一个人右边icon:</span><input type="text" name="first_people_right_icon_url" id="first_people_right_icon_url" class="form-control col-xs-8" value="{$item.first_people_right_icon_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第二个人背景色rgb:</span>
                                    <input type="text" name="second_people_r" id="second_people_r" class="form-control col-xs-2" value="{$second_people_r}"/>
                                    <input type="text" name="second_people_g" id="second_people_g" class="form-control col-xs-2" value="{$second_people_g}"/>
                                    <input type="text" name="second_people_b" id="second_people_b" class="form-control col-xs-2" value="{$second_people_b}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第二个人左边icon:</span><input type="text" name="second_people_left_icon_url" id="second_people_left_icon_url" class="form-control col-xs-8" value="{$item.second_people_left_icon_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第二个人右边icon:</span><input type="text" name="second_people_right_icon_url" id="second_people_right_icon_url" class="form-control col-xs-8" value="{$item.second_people_right_icon_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第三个人背景色rgb:</span>
                                    <input type="text" name="third_people_r" id="third_people_r" class="form-control col-xs-2" value="{$third_people_r}"/>
                                    <input type="text" name="third_people_g" id="third_people_g" class="form-control col-xs-2" value="{$third_people_g}"/>
                                    <input type="text" name="third_people_b" id="third_people_b" class="form-control col-xs-2" value="{$third_people_b}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第三个人左边icon:</span><input type="text" name="third_people_left_icon_url" id="third_people_left_icon_url" class="form-control col-xs-8" value="{$item.third_people_left_icon_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">第三个人右边icon:</span><input type="text" name="third_people_right_icon_url" id="third_people_right_icon_url" class="form-control col-xs-8" value="{$item.third_people_right_icon_url}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">其它人列表背景色rgb:</span>
                                    <input type="text" name="other_people_r" id="other_people_r" class="form-control col-xs-2" value="{$other_people_r}"/>
                                    <input type="text" name="other_people_g" id="other_people_g" class="form-control col-xs-2" value="{$other_people_r}"/>
                                    <input type="text" name="other_people_b" id="other_people_b" class="form-control col-xs-2" value="{$other_people_r}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">列表整体背景色rgb:</span>
                                    <input type="text" name="list_people_r" id="list_people_r" class="form-control col-xs-2" value="{$list_people_r}"/>
                                    <input type="text" name="list_people_g" id="list_people_g" class="form-control col-xs-2" value="{$list_people_r}"/>
                                    <input type="text" name="list_people_b" id="list_people_b" class="form-control col-xs-2" value="{$list_people_r}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动规则前的文字背景URL:</span><input type="text" name="front_rule_bg_url" id="front_rule_bg_url" class="form-control col-xs-8" value="{$item.front_rule_bg_url}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动规则前的说明文字:</span>
                                    <input type="text" name="front_rule_text" id="front_rule_text" class="form-control col-xs-2" value="{$item.front_rule_text}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动要求背景色rgb:</span>
                                    <input type="text" name="rule_text_bg_r" id="rule_text_bg_r" class="form-control col-xs-2" value="{$item.rule_text_bg_r}"/>
                                    <input type="text" name="rule_text_bg_g" id="rule_text_bg_g" class="form-control col-xs-2" value="{$item.rule_text_bg_g}"/>
                                    <input type="text" name="rule_text_bg_b" id="rule_text_bg_b" class="form-control col-xs-2" value="{$item.rule_text_bg_b}"/>
                                </div>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-5">
                                    <button id="add_one_c2c_data" type="button" class="btn btn-primary" onclick="addruleLine()">增加一条规则数据</button>
                                </div>
                                <!--div class="form-group col-xs-12">
                                    <span class="label">活动要求文字:</span>
                                    <input type="text" name="rule_text" id="rule_text" class="form-control col-xs-2" value="{$item.rule_text}"/>
                                </div-->
                            </div>
                            <div class="row" id="rule_content" add_line="{$rule_text_count - 1}">
                                <foreach name="item.rule_text" key="key" item="ruletext_item">
                                    <div class="row rule_text_line">
                                        <div class="form-group col-xs-10">
                                            <span class="">规则文字n</span>
                                            <input type="text" name="rule_text_array[]" class="form-control col-xs-8" id="rule_text_array_{$key}" value="{$ruletext_item}"/>
                                        </div>
                                    </div>
                                </foreach>
                            </div>
                            <div class="row">
                                <div class="form-group col-xs-12">
                                    <span class="label">活动要求文字色rgb:</span>
                                    <input type="text" name="rule_text_r" id="rule_text_r" class="form-control col-xs-2" value="{$item.rule_text_r}"/>
                                    <input type="text" name="rule_text_g" id="rule_text_g" class="form-control col-xs-2" value="{$item.rule_text_g}"/>
                                    <input type="text" name="rule_text_b" id="rule_text_b" class="form-control col-xs-2" value="{$item.rule_text_b}"/>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-5">
                                    <span class="label">是否有特殊任务:</span>
                                    <select name="is_have_special_mission" id="is_have_special_mission">
                                        <option value="0" <if condition="$item.is_have_special_mission eq 0">selected="selected"</if> >没有</option>
                                        <option value="1" <if condition="$item.is_have_special_mission eq 1">selected="selected"</if> >有</option>
                                    </select>
                                </div>
                            </div>

                            <div class="row">
                                <div class="form-group col-xs-5">
                                    <button id="add_one_mission_data" type="button" class="btn btn-primary" onclick="addmissionLine()">增加一条特别任务数据</button>
                                </div>
                            </div>

                            <div class="row" id="mission_content" add_line="{$mission_count - 1}">
                                <foreach name="item.special_mission_tml_data" key="key" item="mission_item">
                                    <div class="row misson_line">
                                        <div class="form-group col-xs-2">
                                            <span class="">类型</span>
                                            <select name="mission_type_array[]" id="mission_type_array_{$key}">
                                                <option value="0" <if condition="$mission_item.type eq 1">selected="selected"</if> >拉人任务</option>
                                            </select>
                                        </div>
                                        <div class="form-group col-xs-3">
                                            <span class="label">数值:</span>
                                            <input type="text" name="mission_value_array[]" id="mission_value_array_{$key}" class="form-control col-xs-2" value="{$mission_item.value}"/>
                                        </div>
                                        <div class="form-group col-xs-4">
                                            <span class="label">限时（分钟）:</span>
                                            <input type="text" name="mission_limit_time_array[]" id="mission_limit_time_array_{$key}" class="form-control col-xs-2" value="{$mission_item.limit_time}"/>
                                        </div>

                                        <div class="form-group col-xs-3">
                                            <span class="label">减钱:</span>
                                            <input type="text" name="mission_dec_money_array[]" id="mission_dec_money_array_{$key}" class="form-control col-xs-2" value="{$mission_item.dec_money}"/>
                                        </div>

                                        <div class="form-group col-xs-3">
                                            <span class="label">触发条件值:</span>
                                            <input type="text" name="mission_trigger_array[]" id="mission_trigger_array_{$key}" class="form-control col-xs-2"  value="{$mission_item.trigger}"/>
                                        </div>

                                    </div>
                                </foreach>
                            </div>

                            <input type="hidden" value="{$item.id}" name="id" />

                            <div class="modal-footer">
                                <button id="form_save" class="btn btn-primary" >保存</button>
                            </div>
                    </form>
                </div>
            </div>

        </div>
    </div>
</div>
</div>
<div id="config-tool" class="closed">
    <a id="config-tool-cog">
        <i class="fa fa-cog"></i>
    </a>

    <div id="config-tool-options">
        <h4>Layout Options</h4>
        <ul>
            <li>
                <div class="checkbox-nice">
                    <input type="checkbox" id="config-fixed-header"/>
                    <label for="config-fixed-header">
                        Fixed Header
                    </label>
                </div>
            </li>
