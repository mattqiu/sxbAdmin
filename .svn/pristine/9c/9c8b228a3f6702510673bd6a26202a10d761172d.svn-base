        $order = "sqty desc";
        switch($sort){
            case '1':
                $order = "op.product_id desc";
            break;
            case '2':
                $order = "op.product_id asc";
            break;
            case '3':
                $order = "sqty asc";
            break;
            case '4':
                $order = "sqty desc";
            break;
            case '5':
                $order = "stm asc";
            break;
            case '6':
                $order = "stm desc";
