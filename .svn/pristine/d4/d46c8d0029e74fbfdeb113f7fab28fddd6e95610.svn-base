                                                            <option value="1" <if condition="$post.time eq 1">selected</if>>----取出时间----</option>
                                                            <option value="2" <if condition="$post.time eq 2">selected</if>>----导入时间----</option>
                                                            <option value="3" <if condition="$post.time eq 3">selected</if>>----打印时间----</option>
